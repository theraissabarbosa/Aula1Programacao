#include <iostream>
#include <string>

using namespace std;
int main() {
	string namec; //declarando variaveis - string eh caracteres ilimitados
	int age;
	cout<< "Hello user \n"; //mostrar msg no prompt
	cout<< "Insira sua nome completo:";
	getline(cin,namec);
	
	cout<< "Insira sua idade:";
	cin>> age;
	
	cout<< "\n Seu nome eh:" << namec;
	cout<< "\n Sua idade eh:" << age;
	return 0;
}
